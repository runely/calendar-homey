        "id": "event_starts",
        "title": {
          "en": "Event starts",
          "no": "En avtale starter",
          "nl": "Event start",
          "de": "Ereignis beginnt"
        },
        "tokens": [
          {
            "name": "event_name",
            "type": "string",
            "title": {
              "en": "Title",
              "no": "Tittel",
              "nl": "Titel",
              "de": "Titel"
            },
            "example": {
              "en": "Pick up in kindergarden",
              "no": "Hente i barnehagen",
              "nl": "Kinderen ophalen",
              "de": "Im Kindergarten abholen"
            }
          },
          {
            "name": "event_description",
            "type": "string",
            "title": {
              "en": "Description",
              "no": "Beskrivelse",
              "nl": "Omschrijving",
              "de": "Beschreibung"
            },
            "example": {
              "en": "Pick up kids in kindergarden",
              "no": "Hente barna fra barnehagen",
              "nl": "Haal de kinderen van school op",
              "de": "Kinder im Kindergarten abholen"
            }
          },
          {
            "name": "event_location",
            "type": "string",
            "title": {
              "en": "Location",
              "no": "Sted",
              "nl": "Lokatie",
              "de": "Ort"
            },
            "example": {
              "en": "Kindergarden street, 30",
              "no": "Barnehagegata, 30",
              "nl": "Schoolstraat 30",
              "de": "Kindergartenstraße 30"
            }
          },
          {
            "name": "event_duration_readable",
            "type": "string",
            "title": {
              "en": "Duration",
              "no": "Varighet",
              "nl": "Looptijd",
              "de": "Dauer"
            },
            "example": {
              "en": "1.5 hour",
              "no": "1.5 time",
              "nl": "1.5 uur",
              "de": "1.5 Stunden"
            }
          },
          {
            "name": "event_duration",
            "type": "number",
            "title": {
              "en": "Duration in minutes",
              "no": "Varighet i minutter",
              "nl": "Looptijd in minuten",
